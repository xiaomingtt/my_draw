<canvas class="canvas" id="canvas" canvas-id="canvas" disable-scroll="true" bindtouchstart="canvasStart" bindtouchmove="canvasMove" bindtouchend="canvasEnd" touchcancel="canvasEnd" binderror="canvasIdErrorCallback">



</canvas>

<view class="yanse">
  <block wx:for="{{colors}}" wx:key="index">
    <view style="background-color:{{item}};" class="flex-item" data-id="{{index}}" bindtap='checkcolor'>
      <image src="../../images/ok.png" style="width:2.5vh;height:2.5vh;margin:1vh auto;" wx:if="{{xuanzhongcolor==index}}"></image>
    </view>
  </block>
  <view style="background:url(data:image/png;base64,) no-repeat;background-size:100% 100%;"
    class="flex-item" data-id="999" bindtap='checkcolor'>
    <image src="../../images/ok.png" style="width:2.5vh;height:2.5vh;margin:1vh auto;" wx:if="{{xuanzhongcolor=='999'}}"></image>
  </view>
</view>
<view style="width:100vw;height:6.5vh;background-color:#bdbdbd;">
  <view style="width:85%;height:100%;float:left;">
    <slider bindchange="slider4change" min="1" max="20" show-value block-size="18" value="{{cuxi}}" style="height:100%;margin:0.5vh 0;padding:8rpx 30rpx;" />
  </view>
  <view style="margin:-10rpx 8rpx auto auto;float:right;width:12%;height:130%;background:url(data:image/png;base64,) no-repeat;background-size:100% 100%;"
    bindtap='xiangpi' data-id='888'>
    <image src="../../images/ok.png" style="float:right;width:3vh;height:3vh;margin:30rpx 25rpx;" wx:if="{{xuanzhongcolor=='888'}}"></image>
  </view>
</view>

<view class="inputview">
  <input placeholder="请输入物品名，如：手机" maxlength="6" bindinput='inputtitle' value="{{tit}}" confirm-type="next"></input>

  <input placeholder="请输入提示，如：通讯工具" maxlength="10" bindinput='inputtishi' value="{{ti}}" confirm-type="done"></input>
</view>
<view style="width:100vw;height:9vh;background-color:#f0f0f0;margin-top:3vh;">
  <view class="anniu">

    <button size='mini' type='primary' wx:if="{{isdraw}}" style="width:100%;height:100%;font-size:36rpx;" bindtap='setSign'>生 成</button>
    <button size='mini' type='primary' wx:if="{{!isdraw}}" style="width:100%;height:100%;font-size:36rpx;" bindtap='newdraw'>再 来</button>
  </view>
  <view class="anniu">
    <button size='mini' type='primary' style="width:100%;height:100%;font-size:36rpx;" bindtap='cleardraw' disabled="{{!jinshare}}">清 除</button>
  </view>
  <view class="anniu">
    <button size='mini' type='primary' open-type="share" style="width:100%;height:100%;font-size:36rpx;" disabled="{{jinshare}}">分 享</button>
  </view>
</view>








<view class="colo" wx:if="{{showcolor}}" style="position:fixed;width:700rpx;height:260rpx;top:700rpx;left:25rpx;background-color:{{secai}};border-radius:20rpx;text-align:center;z-index:99999;">
  <slider bindchange="slider4co" bindchanging="slider4co" data-color="red" min="0" max="255" show-value value="{{r}}" block-size="13" />
  <slider bindchange="slider4co" bindchanging="slider4co" data-color="green" min="0" max="255" show-value value="{{g}}" block-size="13" />
  <slider bindchange="slider4co" bindchanging="slider4co" data-color="blue" min="0" max="255" show-value value="{{b}}" block-size="13" />
  <button size='mini' bindtap='closecolor'>确定</button>
</view>